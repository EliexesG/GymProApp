                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Usuario.Identificacion, Usuario.Nombre, Usuario.Fotografia, Usuario.Correo, Usuario.Telefono, FacturaEncabezado.FechaProximoPago, DATEDIFF(day, FacturaEncabezado.FechaProximoPago, GETDATE()) AS DiasMorosidad
FROM     FacturaEncabezado INNER JOIN
                  Usuario ON FacturaEncabezado.IdentificacionUsuario = Usuario.Identificacion
WHERE  (Usuario.CodigoTipoUsuario = 1) AND (Usuario.Activo = 1) AND (FacturaEncabezado.FechaProximoPago =
                      (SELECT MAX(FechaProximoPago) AS Expr1
                       FROM      FacturaEncabezado AS FacturaEncabezado_1
                       WHERE   (Activo = 1) AND (IdentificacionUsuario = Usuario.Identificacion))) AND (FechaProximoPago &lt;= GETDATE())</CommandText>
